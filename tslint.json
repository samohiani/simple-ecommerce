{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotemark": [true, "double"],
    "semicolon": [true, "always"],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
